{
  "compilerOptions": {
    "plugins": [{ "transform": "ts-nameof", "type": "raw" }],
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    
  },
  "include": [
    "src", "public/switch/swintch_from_classwork/App.tsx", "public/switch/swintch_from_classwork/Button.tsx"
  ]
  }
